<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema' xmlns:tns='urn:riv:itintegration:messagebox:GenerateUrlResponder:1' xmlns:core='urn:riv:itintegration:messagebox:1' targetNamespace='urn:riv:itintegration:messagebox:GenerateUrlResponder:1' elementFormDefault='qualified' attributeFormDefault='unqualified' version='1.0'>
  <xs:import schemaLocation='../../core_components/itintegration_messagebox_1.0.xsd' namespace='urn:riv:itintegration:messagebox:1' />
  <xs:element name='GenerateUrl' type='tns:GenerateUrlType' />
  <xs:element name='GenerateUrlResponse' type='tns:GenerateUrlResponseType' />

  <xs:complexType name='GenerateUrlType'>
      <xs:sequence>
          <xs:annotation>
              <xs:documentation>
                  The HSA-Id of the consumer of the List/Get/Delete interfaces will be embedded in the
                  URL, and used to tag each incoming message with the hsa-id that will be presented by
                  the consumer for whom the message is intended.
              </xs:documentation>
          </xs:annotation>
          <xs:element name='consumerHsaId' type='xs:string'/>
          <xs:any namespace='##other' processContents='lax' minOccurs='0' maxOccurs='unbounded'/>
      </xs:sequence>
  </xs:complexType>

  <xs:complexType name='GenerateUrlResponseType'>
      <xs:sequence>
          <xs:annotation>
              <xs:documentation>
                  If the resultCode is ResultCodeEnum:OK, the messageBoxUrl contains
                  the url to the message box service for the specified consumerHsaId,
                  otherwise it is empty.
              </xs:documentation>
          </xs:annotation>
          <xs:element name='result' type='core:ResultType'/>
          <xs:element name='messageBoxUrl' type='xs:string'/>
          <xs:any namespace='##other' processContents='lax' minOccurs='0' maxOccurs='unbounded'/>
      </xs:sequence>
  </xs:complexType>

</xs:schema>