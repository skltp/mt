<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'
           xmlns:mb='urn:riv:itintegration:messagebox:1'
           xmlns:reg='urn:riv:itintegration:registry:1'
           xmlns:tns='urn:riv:itintegration:messagebox:ListMessagesResponder:1'
           targetNamespace='urn:riv:itintegration:messagebox:ListMessagesResponder:1'
           elementFormDefault='qualified' attributeFormDefault='unqualified'
           version='1.0'>
    <xs:import schemaLocation='../../core_components/itintegration_messagebox_1.0.xsd'
               namespace='urn:riv:itintegration:messagebox:1'/>
    <xs:element name='ListMessages' type='tns:ListMessagesType'/>
    <xs:element name='ListMessagesResponse' type='tns:ListMessagesResponseType'/>

    <xs:complexType name='ListMessagesType'>
        <xs:sequence>
            <xs:element name='systemId' type='reg:LogicalAddressType'/>
            <xs:element name='serviceContractTypes' type='reg:ServiceContractType' minOccurs="0" maxOccurs="unbounded"/>
            <xs:any namespace='##other' processContents='lax' minOccurs='0' maxOccurs='unbounded'/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name='ListMessagesResponseType'>
        <xs:sequence>
            <xs:element name='resultCode' type='mb:ResultCodeEnum'/>
            <xs:element name='remainingMessages' type='xs:integer'/>
            <xs:element name='messageMetas' type='mb:MessageMetaType' minOccurs='0' maxOccurs='unbounded'/>
            <xs:any namespace='##other' processContents='lax' minOccurs='0' maxOccurs='unbounded'/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>